          required: true
          schema:
            type: string
            example: basic
        - name: currency
          in: query
          description: The currency of the price information. Options usd, btc. Only used in conjunction with includePrice
          required: true
          schema:
            type: string
            example: USD
        - name: includePrice
          in: query
          description: Indicates whether or not to include price data with the results
          required: true
          schema:
            type: boolean
            example: true
