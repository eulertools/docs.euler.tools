          required: false
          schema:
            type: string
            example: timestamp
        - name: timeFormat
          in: query
          description: Time format of the timestamps in the return payload. ([Defaults] milliseconds | ms* | iso | iso8611 | hr | human_readable)
          required: false
          schema:
            type: string
            example: iso
      responses:
